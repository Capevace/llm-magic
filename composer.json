{
    "name": "mateffy/llm-magic",
    "description": "AI toolkit for Laravel",
    "authors": [
        {
            "name": "Lukas Mateffy",
            "email": "github@mateffy.me"
        }
    ],
    "type": "library",
    "require": {
        "aws/aws-sdk-php": "^3.315",
        "ext-fileinfo": "*",
        "google-gemini-php/client": "^1.0",
        "greghunt/partial-json": "^1.1",
        "illuminate/collections": "*",
        "nlp-tools/nlp-tools": "^0.1.3",
        "nyholm/psr7": "^1.8",
        "openai-php/client": "^0.10.2",
        "php": "^8.3",
        "swaggest/json-schema": "^0.12.42",
        "symfony/http-client": "^7.0"
    },
    "require-dev": {
        "symplify/monorepo-builder": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "Mateffy\\Magic\\": "packages/llm-magic/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "replace": {
        "mateffy/llm-magic": "1.0.0-rc1"
    }
}
