{
    "name": "mateffy/llm-magic",
    "description": "A package to magically import complex data structures using LLMs and AI methods.",
    "type": "library",
    "license": "AGPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Mateffy\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lukas Mateffy",
            "email": "github@mateffy.me"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.3",
        "openai-php/client": "^0.10.2",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8",
        "greghunt/partial-json": "^1.1",
        "ext-fileinfo": "*",
        "illuminate/collections": "*",
        "swaggest/json-schema": "^0.12.42",
        "aws/aws-sdk-php": "^3.315",
        "nlp-tools/nlp-tools": "^0.1.3",
        "google-gemini-php/client": "^1.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mateffy\\Magic\\LlmMagicServiceProvider"
            ]
        }
    }
}
